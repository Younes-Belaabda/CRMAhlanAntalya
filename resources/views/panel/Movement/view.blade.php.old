            return $collection->where("status",1)->count();
            }
            if($type == "paid"){
            return $collection->where("status",0)->count();
            }
            if($type == "net2"){
                return $collection->where("paybyus" , "0")->where("status",1)->count();
            }
            if($type == "net"){
                return $collection->where("paybyus" , "1")->where("status",1)->count();
